{% extends 'cursus/cursus-subnav.html.twig' %}
{% form_theme form 'form/bootstrap_4_horizontal_layout.html.twig' %}
{% block title %}
  Nouveau cursus
{% endblock %}

{% block content %}

{{ form_start(form) }}
    {{ form_row(form.label) }}

    <h3>Tags</h3>
    <ul class="tags">
        {# iterate over each existing tag and render its only field: name #}
        {% for elemFormation in form.elementsFormations %}
            <li>{{ form_row(elemFormation.sigle) }}</li>
        {% endfor %}
    </ul>
{{ form_end(form) }}




{% endblock %}
